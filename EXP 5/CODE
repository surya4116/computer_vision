import cv2
image = cv2.imread("C:\\Users\\lenovo\\OneDrive\\Pictures\\Screenshots\\Screenshot 2026-02-11 142415.png")   
if image is None:
    print("Error: Unable to load image.")
    exit()
clockwise = cv2.rotate(image, cv2.ROTATE_90_CLOCKWISE)
counter_clockwise = cv2.rotate(image, cv2.ROTATE_90_COUNTERCLOCKWISE)
cv2.imshow("Original", image)
cv2.imshow("Clockwise", clockwise)
cv2.imshow("Counter Clockwise", counter_clockwise)
cv2.waitKey(0)
cv2.destroyAllWindows()
