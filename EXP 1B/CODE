import cv2
image = cv2.imread("C:\\Users\\lenovo\\OneDrive\\Pictures\\Screenshots\\Screenshot 2026-02-11 105027.png")   # give your image path
if image is None:
    print("Error: Unable to load image.")
    exit()
blurred = cv2.GaussianBlur(image, (15, 15), 0)
cv2.imshow("Original Image", image)
cv2.imshow("Gaussian Blurred Image", blurred)
cv2.imwrite("blurred_output.jpg", blurred)

cv2.waitKey(0)
cv2.destroyAllWindows()
